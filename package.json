{
  "name": "@cmbr/sb-patterns-preset",
  "version": "0.0.3",
  "description": "A storybook preset add-on for using patterns. ",
  "keywords": [
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencer-leopold/sb-patterns-preset.git"
  },
  "author": "Spencer Leopold <spencer.leopold@cmbr.co>",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preset": {
      "require": "./dist/preset.js",
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts"
    },
    "./configuration": {
      "require": "./dist/configuration.js",
      "import": "./dist/configuration.mjs",
      "types": "./dist/configuration.d.ts"
    },
    "./storiesLoader": {
      "require": "./dist/storiesLoader.js",
      "import": "./dist/storiesLoader.mjs",
      "types": "./dist/storiesLoader.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "build": "tsup-node",
    "build:watch": "yarn build --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.34",
    "@types/webpack": "^5.28.2",
    "@types/webpack-env": "^1.18.1",
    "@vitejs/plugin-react": "^3.1.0",
    "auto": "^10.3.0",
    "boxen": "^5.0.1",
    "dedent": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/blocks": "^7.0.0",
    "@storybook/components": "^7.0.0",
    "@storybook/core-events": "^7.0.0",
    "@storybook/manager-api": "^7.0.0",
    "@storybook/preview-api": "^7.0.0",
    "@storybook/theming": "^7.0.0",
    "@storybook/types": "^7.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Storybook Patterns",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "dependencies": {
    "@cmbr/sb-patterns": "^0.0.2"
  },
  "bugs": {
    "url": "https://github.com/spencer-leopold/sb-patterns-preset/issues"
  },
  "homepage": "https://github.com/spencer-leopold/sb-patterns-preset#readme"
}
